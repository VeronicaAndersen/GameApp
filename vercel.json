{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./ ':!Gemfile' ':!Gemfile.lock' ':!Podfile' ':!Podfile.lock' ':!ios/' ':!android/'",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
